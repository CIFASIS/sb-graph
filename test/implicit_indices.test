// model implicit_indices
//   Real a[100][100], b[100];
// equation
//   for i in 1:100 loop
//     a[i] = b;
//   end loop;
// end implicit_indices

dims = 2

V %= {[0:1:99]x[0:1:99], [101:1:200]x[200:1:299], [100:1:100]x[100:1:199]};
Vmap %= <<{[100:1:100]x[100:1:199]} -> 0*x+2|0*x+2, {[0:1:199]x[0:1:199]} -> 0*x+1|0*x+1
          , {[101:1:200]x[200:1:299]} -> 0*x+3|0*x+3>>;
map1 %= <<{[0:1:99]x[0:1:99]} -> 1*x+101|1*x+200, {[100:1:199]x[100:1:199]} -> 1*x+1|1*x+100>>;
map2 %= <<{[100:1:199]x[100:1:199]} -> 0*x+100|1*x+0, {[0:1:99]x[0:1:99]} -> 1*x+0|1*x+0>>;
Emap %= <<{[0:1:99]x[0:1:99]} -> 0*x+1|0*x+1, {[100:1:199]x[100:1:199]} -> 0*x+2|0*x+2>>;

matching(
  V %= {[0:1:99]x[0:1:99], [101:1:200]x[200:1:299], [100:1:100]x[100:1:199]};
  Vmap %= <<{[100:1:100]x[100:1:199]} -> 0*x+2|0*x+2, {[0:1:199]x[0:1:199]} -> 0*x+1|0*x+1
            , {[101:1:200]x[200:1:299]} -> 0*x+3|0*x+3>>;
  map1 %= <<{[0:1:99]x[0:1:99]} -> 1*x+101|1*x+200, {[100:1:199]x[100:1:199]} -> 1*x+1|1*x+100>>;
  map2 %= <<{[100:1:199]x[100:1:199]} -> 0*x+100|1*x+0, {[0:1:99]x[0:1:99]} -> 1*x+0|1*x+0>>;
  Emap %= <<{[0:1:99]x[0:1:99]} -> 0*x+1|0*x+1, {[100:1:199]x[100:1:199]} -> 0*x+2|0*x+2>>;
)
