// SCC of Test1 from the paper

N = 100

F1a = N
F2b = N+F1a
F3x = N+F2b

E1 = N // F1a - F2b
E2 = N+E1 // F2b - F1a
E3 = N+E2 // F3x - F1a

off1d = r(F2b, 1)-r(E1, 1)
off2d = r(F1a, 1)-r(E2, 1)
off3d = r(F1a, 1)-r(E3, 1)

off1b = r(F1a, 1)-r(E1, 1)
off2b = r(F2b, 1)-r(E2, 1)
off3b = r(F3x, 1)-r(E3, 1)

V %= {[1:1:F1a], [F1a+1:1:F2b], [F2b+1:1:F3x]};
Vmap %= <<{[1:1:F1a]} -> 0*x+1, {[F1a+1:1:F2b]} -> 0*x+2, {[F2b+1:1:F3x]} -> 0*x+3>>;
mapB %= <<{[1:1:E1]} -> 1*x+off1b, {[E1+1:1:E2]} -> 1*x+off2b, {[E2+1:1:E3]} -> 1*x+off3b>>;
mapD %= <<{[1:1:E1]} -> 1*x+off1d, {[E1+1:1:E2]} -> 1*x+off2d, {[E2+1:1:E3]} -> 1*x+off3d>>;
Emap %= <<{[1:1:E1]} -> 0*x+1, {[E1+1:1:E2]} -> 0*x+2, {[E2+1:1:E3]} -> 0*x+3>>;

scc(
V %= {[1:1:F1a], [F1a+1:1:F2b], [F2b+1:1:F3x]};
Vmap %= <<{[1:1:F1a]} -> 0*x+1, {[F1a+1:1:F2b]} -> 0*x+2, {[F2b+1:1:F3x]} -> 0*x+3>>;
mapB %= <<{[1:1:E1]} -> 1*x+off1b, {[E1+1:1:E2]} -> 1*x+off2b, {[E2+1:1:E3]} -> 1*x+off3b>>;
mapD %= <<{[1:1:E1]} -> 1*x+off1d, {[E1+1:1:E2]} -> 1*x+off2d, {[E2+1:1:E3]} -> 1*x+off3d>>;
Emap %= <<{[1:1:E1]} -> 0*x+1, {[E1+1:1:E2]} -> 0*x+2, {[E2+1:1:E3]} -> 0*x+3>>;
)
