CC(
  V %= {[1:1:N1], [N1+1:1:N2], [N2+1:1:N3], [N3+1:1:N4], [N4+1:1:N5], [N5+1:1:N6], [N6+1:1:N7], [N7+1:1:N8]};
  Vmap %= <<>>; 
  map1 %= <<{[1:1:1]} -> 0*x+o1, {[3:1:3]} -> 0*x+o2, {[5:1:N1+3]} -> 1*x+o3, {[N4+1:1:N5]} -> 1*x-o4, {[N5+1:1:N6]} -> 1*x-o5, {[N8+1:1:N9]} -> 1*x-o11>>;
  map2 %= <<{[1:1:1]} -> 0*x+o6, {[3:1:3]} -> 0*x+o7, {[5:1:N1+3]} -> 1*x+o8, {[N4+1:1:N5]} -> 1*x+0, {[N5+1:1:N6]} -> 0*x+o10, {[N8+1:1:N9]} -> 1*x-o12>>;
  Emap %= <<>>;
)
