N = 1000
N1 = N
N2 = N1+N
N3 = N2+1
N4 = N3+1
N5 = N4+N
N6 = N5+N
N7 = N6+1
N8 = N7+1
N9 = N8+N-1

o1 = N8
o2 = N7
o3 = 2*N-2
o4 = N+2
o5 = 3*N+2
o11 = 4*N+4

o6 = N5+1
o7 = N4
